{
  "name": "Task",
  "plural": "Tasks",
  "base": "Base",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mssql": {
      "table": "Tasks"
    }
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "weight": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "budget": {
      "type": "number",
      "required": true,
      "description": [
        "budget in man workhour"
      ],
      "default": 1
    },
    "budgetused": {
      "type": "number",
      "required": true,
      "description": [
        "budget used in man workhour"
      ],
      "default": 0
    },
    "planstart": {
      "type": "date",
      "required": true,
      "default": "$now",
      "mssql": {
        "dataType": "datetime2"
      }
    },
    "planend": {
      "type": "date",
      "required": true,
      "default": "$now",
      "mssql": {
        "dataType": "datetime2"
      }
    },
    "actualstart": {
      "type": "date",
      "required": true,
      "default": "$now",
      "mssql": {
        "dataType": "datetime2"
      }
    },
    "actualend": {
      "type": "date",
      "required": true,
      "default": "$now",
      "mssql": {
        "dataType": "datetime2"
      }
    },
    "remark": {
      "type": "string",
      "default": ""
    },
    "status": {
      "type": "string",
      "required": true,
      "default": ""
    }
  },
  "validations": [],
  "relations": {
    "backlog": {
      "type": "belongsTo",
      "model": "Backlog",
      "foreignKey": "backlogId"
    },
    "project": {
      "type": "belongsTo",
      "model": "Project",
      "foreignKey": "projectId"
    },
    "assignments": {
      "type": "hasMany",
      "model": "Assignment",
      "foreignKey": "taskId"
    }
  },
  "acls": [],
  "methods": {}
}